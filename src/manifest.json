{
    "manifest_version": 3,
    "name": "Risk Avoidance",
    "description": "A web browser extension (Manifest V3) for risk avoidance in Bluesky",
    "version": "1.0",
    "content_scripts": [
        {
          "matches": ["https://bsky.app/"],
          "js": ["content/index.js"]
        }
    ],
    "host_permissions": ["<all_urls>"]
  }